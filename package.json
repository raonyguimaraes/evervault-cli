{
  "name": "evervault-cli",
  "description": "CLI interface for managing Evervault Lambdas",
  "version": "0.1.2",
  "author": "Shane Curran",
  "bin": {
    "evervault": "./bin/run"
  },
  "bugs": "https://github.com/evervault/evervault-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.10",
    "@oclif/config": "^1.12.6",
    "@oclif/plugin-help": "^2.1.6",
    "chalk": "^2.4.2",
    "cli-table": "^0.3.1",
    "cli-ux": "^5.3.2",
    "dotenv": "^6.2.0",
    "listr": "^0.14.3",
    "log-symbols": "^3.0.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.0",
    "eslint": "^5.14.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^8.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/config.json"
  ],
  "homepage": "https://github.com/evervault/evervault-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "evervault",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "evervault/evervault-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
